{
  "name": "markdown-wysiwyg",
  "displayName": "Markdown WYSIWYG Editor",
  "description": "Notion-like WYSIWYG editor for Markdown files with auto-rendering, bold/italic, blockquotes, footnotes, and multi-column layouts",
  "version": "0.2.0",
  "publisher": "your-publisher-name",
  "repository": {
    "type": "git",
    "url": "https://github.com/paperMoose/CursorNote"
  },
  "bugs": {
    "url": "https://github.com/paperMoose/CursorNote/issues"
  },
  "homepage": "https://github.com/paperMoose/CursorNote/blob/main/markdown-wysiwyg/README.md",
  "keywords": [
    "markdown",
    "wysiwyg",
    "editor",
    "preview",
    "notion"
  ],
  "engines": {
    "vscode": "^1.74.0"
  },
  "categories": ["Other"],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "customEditors": [
      {
        "viewType": "markdownWysiwyg.editor",
        "displayName": "Markdown WYSIWYG Editor",
        "selector": [
          {
            "filenamePattern": "*.md"
          }
        ],
        "priority": "default"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./"
  },
  "devDependencies": {
    "@types/vscode": "^1.74.0",
    "@types/node": "16.x",
    "typescript": "^4.9.4"
  }
}